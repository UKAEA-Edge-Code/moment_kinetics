#!/bin/bash

#SBATCH --mem=16G
#SBATCH --time=1:00:00
#SBATCH --account=ACCOUNT
#SBATCH --partition=serial
#SBATCH --qos=serial
#SBATCH --output=PRECOMPILEDIRslurm-%j.out

set -e

cd $SLURM_SUBMIT_DIR

# Get setup for Julia
source julia.env

echo "precompiling"
bin/julia --project -O3 --check-bounds=no precompile.jl

echo "finished!"
